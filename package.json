{
  "name": "moonraker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "npm": "use-pnpm",
    "yarn": "use-pnpm"
  },
  "scripts": {
    "build:rsa-cli": "cd rsa-cli && cargo build && cd ..",
    "build": "forge build && pnpm run build:rsa-cli",
    "fmt": "forge fmt",
    "fmt:check": "forge fmt --check",
    "solhint": "solhint --fix --config ./.solhint.json 'src/**/*.sol'",
    "solhint:check": "solhint --config ./.solhint.json 'src/**/*.sol'",
    "lint": "npm run fmt && npm run solhint",
    "lint:check": "pnpm run fmt:check && pnpm run solhint:check",
    "size": "forge clean && forge build --sizes --skip test --skip '*/Mock*.sol'",
    "test": "forge test -vvv"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "solhint-community": "^3.7.0"
  }
}